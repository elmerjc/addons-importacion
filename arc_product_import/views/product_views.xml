<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="product_template_import_tree" model="ir.ui.view">
    <field name="name">product.template.import.tree</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_tree_view"/>
    <field name="arch" type="xml">
      <field name="name" position="before">
        <field name="minicode" optional="show"/>
      </field>
      <field name="name" position="after">
        <field name="model" optional="hide"/>
        <field name="tecnology" optional="hide"/>
      </field>
    </field>
  </record>

  <record id="product_template_import_form" model="ir.ui.view">
    <field name="name">product.template.import.form</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_form_view"/>
    <field name="arch" type="xml">
      <field name="uom_po_id" position="after">
        <field name="minicode"/>
        <field name="model"/>
        <field name="tecnology"/>
      </field>
    </field>
  </record>
</odoo>